<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cvmakerr.advance.cvmakepro">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/appicon"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@drawable/appicon"
        android:supportsRtl="true"
        android:theme="@style/Theme.CvmakerAdvance">
        <activity android:name=".Template_Selector"></activity>
        <activity
            android:name=".CvTemplates.Design1.CreateMenu"
            android:launchMode="singleTask" />
        <activity
            android:name=".CvTemplates.Design1.design1_temp2"
            android:launchMode="singleTask" />
        <activity android:name=".MainActivity" />
        <activity android:name=".Main_Ui.MainHome" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>